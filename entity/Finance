package graduate;

import java.sql.Date;

public class Finance {
	private String teamName;
	private String type;
	private int amount;
	private Date time;
	private String year;
	private String month;
	private String remark;
	public Finance(String teamName,String type,int amount,Date time,String year,String month,String remark) {
		super();
		this.teamName = teamName;
		this.type = type;
		this.amount = amount;
		this.time = time;
		this.year = year;
		this.month = month;
		this.remark = remark;
	}
	public String getTeamName() {
		return teamName;
	}
	public void setTeamName(String teamName) {
		this.teamName = teamName;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public Date getTime() {
		return time;
	}
	public void setTime(Date time) {
		this.time = time;
	}
	public String getYear() {
		return year;
	}
	public void setYear(String year) {
		this.year = year;
	}
	public String getMonth() {
		return month;
	}
	public void setMonth(String month) {
		this.month = month;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	
}
