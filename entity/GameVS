package graduate;

import java.sql.Date;

public class GameVS {
	private String teamA;
	private String teamB;
	private Date date;
	
	public GameVS(String teamA,String teamB,Date date) {
		super();
		this.teamA = teamA;
		this.teamB = teamB;
		this.date = date;
	}

	public String getTeamA() {
		return teamA;
	}

	public void setTeamA(String teamA) {
		this.teamA = teamA;
	}

	public String getTeamB() {
		return teamB;
	}

	public void setTeamB(String teamB) {
		this.teamB = teamB;
	}

	public Date getDate() {
		return date;
	}

	public void setGroup(Date date) {
		this.date = date;
	}
}
